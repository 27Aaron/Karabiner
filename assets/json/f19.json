{
    "description": "Change right_command key to command+control+option+shift. (Post f19 key when pressed alone)",
    "manipulators": [
        {
            "from": {
                "key_code": "right_command",
                "modifiers": {
                    "optional": [
                        "any"
                    ]
                }
            },
            "to": [
                {
                    "key_code": "left_shift",
                    "modifiers": [
                        "left_command",
                        "left_control",
                        "left_option"
                    ]
                }
            ],
            "to_if_alone": [
                {
                    "key_code": "f19"
                }
            ],
            "type": "basic"
        }
    ]
}